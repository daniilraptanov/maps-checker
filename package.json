{
  "name": "todocharts",
  "version": "1.0.0",
  "description": "",
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && node dist/src/app.js",
    "server": "nodemon --config nodemon.json src/app.ts",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniilraptanov/ToDoCharts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daniilraptanov/ToDoCharts/issues"
  },
  "homepage": "https://github.com/daniilraptanov/ToDoCharts#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "node-sass": "^7.0.1",
    "rxjs": "^7.5.5",
    "uuid": "^8.3.2"
  }
}
